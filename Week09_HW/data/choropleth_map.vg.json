{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Number of Visitors to Australia in 2024",
  "width": 800,
  "height": 400,
  "projection": { "type": "equirectangular"},
  "layer": [
    {
      "data": {
        "url": "data/output.json",
        "format": { "type": "topojson", "feature": "states" }
      },
      "transform": [
        {
          "lookup": "properties.STATE_NAME",
          "from": {
            "data": {
              "url": "data/visitors_2024_states.csv"
            },
            "key": "State",
            "fields": ["Percantage Over Total %"]
          }
        }
      ],
      "mark": { "type": "geoshape" },
      "encoding": {
        "color": {
          "field": "Percantage Over Total %",
          "type": "quantitative",
          "scale": { "type": "log" }
        },
        "tooltip": [
          {
            "field": "properties.STATE_NAME",
            "type": "nominal",
            "title": "State"
          },
          { "field": "Percantage Over Total %", "type": "quantitative" }
        ]
      }
    },
    {
      "data": {
        "url": "data/output.json",
        "format": { "type": "topojson", "feature": "ne_110m_graticules_30" }
      },
      "mark": {
        "type": "geoshape",
        "stroke": "black",
        "strokeWidth": 1
      },
      "transform": [
        {
          "lookup": "properties.STATE_NAME",
          "from": {
            "data": {
              "url": "data/visitors_2024_states.csv"
            },
            "key": "State",
            "fields": ["Number of Visitors"]
          }
        }
      ]
    }
  ]
}
